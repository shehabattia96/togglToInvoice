<div class="ritz grid-container" dir="ltr">
    <table class="waffle no-grid" cellspacing="0" cellpadding="0">
        <tbody>
            <tr style="height: 25px">
                <td class="s0"></td>
                <td class="s1" dir="ltr" colspan="3">{{headerData.fromCompany.clientName}}</td>
                <td class="s2" dir="ltr" colspan="3">Invoice</td>
            </tr>
            <tr style="height: 20px">
                <td class="s0"></td>
                <td class="s3" dir="ltr" colspan="3">{{headerData.fromCompany.address}}</td>
                <td class="s4" dir="ltr" colspan="3">Submitted on {{headerData.invoicePreparedOnDate}}</td>
            </tr>
            <tr style="height: 20px">
                <td class="s0"></td>
                <td class="s5" dir="ltr" colspan="3">{{headerData.fromCompany.cityCountryZipcode}}</td>
                <td class="s3"></td>
                <td class="s3"></td>
                <td class="s3"></td>
            </tr>
            <tr style="height: 20px">
                <td class="s6"></td>
                <td class="s3" dir="ltr" colspan="2">{{headerData.fromCompany.phoneNumber}}</td>
                <td class="s3"></td>
                <td class="s3"></td>
                <td class="s3"></td>
                <td class="s3"></td>
            </tr>
            <tr style="height: 20px">
                <td class="s6"></td>
                <td class="s8" dir="ltr" colspan="2"></td>
                <td class="s0" colspan="2"></td>
                <td class="s0" colspan="2"></td>
            </tr>
            <tr style="height: 23px">
                <td class="s9"></td>
                <td class="s10" dir="ltr" colspan="2">Invoice for</td>
                <td class="s10" dir="ltr" colspan="2">Project</td>
                <td class="s10" dir="ltr" colspan="2">Invoice #</td>
            </tr>
            <tr style="height: 23px">
                <td class="s6"></td>
                <td class="s3" dir="ltr" colspan="2">{{headerData.toCompany.clientName}}</td>
                <td class="s11" dir="ltr">{{headerData.toCompany.projectName}}</td>
                <td></td>
                <td class="s3" dir="ltr" colspan="2">{{headerData.invoiceNumber}}</td>
            </tr>
            <tr style="height: 23px">
                <td class="s6"></td>
                <td class="s3" dir="ltr" colspan="2">{{headerData.toCompany.address}}</td>
                <td class="s11" dir="ltr"></td>
                <td></td>
                <td class="s3" dir="ltr" colspan="2">Week of {{headerData.invoiceStartDate}} to {{headerData.invoiceEndDate}}</td>
            </tr>
            <tr style="height: 23px">
                <td class="s6"></td>
                <td class="s12 softmerge" dir="ltr">
                    <div class="softmerge-inner" style="width:190px;left:-1px">{{headerData.toCompany.cityCountryZipcode}}</div>
                </td>
                <td class="s13" dir="ltr"></td>
                <td class="s13" dir="ltr" colspan="2"></td>
                <td class="s14" dir="ltr" colspan="2"></td>
            </tr>
            <tr style="height: 20px">
                <td class="s0"></td>
                <td class="s15" colspan="2"></td>
                <td class="s15" colspan="2"></td>
                <td class="s15" colspan="2"></td>
            </tr>
            <tr style="height: 20px">
                <td class="s6"></td>
                <td class="s3"></td>
                <td class="s3"></td>
                <td class="s3"></td>
                <td class="s3"></td>
                <td class="s3"></td>
                <td class="s3"></td>
            </tr>
            <tr style="height: 39px">
                <td class="s6"></td>
                <td class="s18_leftAlign softmerge" dir="ltr">
                    <div class="softmerge-inner" style="width:190px;left:-1px">Description</div>
                </td>
                <td class="s17" dir="ltr"></td>
                <td class="s17" dir="ltr"></td>
                <td class="s18" dir="ltr">Hours</td>
                <td class="s18" dir="ltr">Rate</td>
                <td class="s18" dir="ltr">Total price</td>
            </tr>
            {% set sums = namespace(items=0) %}
            {% set sums.sumHours = 0 %}
            {% set sums.sumAmount = 0 %}
            {% for project in allProjects %}
            {% set lightTheme = ('_light' if loop.index is divisibleby(2) else '') %}
            <tr style="height: 25px">
                <td class="s6"></td>
                <td class="s19{{lightTheme}}" dir="ltr" colspan="3">{{project.name}}</td>
                <td class="s20{{lightTheme}}" dir="ltr">{{summedProjectDurationsInHours[project.id]}}</td>
                <td class="s20{{lightTheme}}" dir="ltr">${{project.hourlyRate}}</td>
                {% set sums.sumHours = sums.sumHours+summedProjectDurationsInHours[project.id] %}
                {% set amount = project.hourlyRate*summedProjectDurationsInHours[project.id] %}
                {% set sums.sumAmount = sums.sumAmount + amount %}
                <td class="s21{{lightTheme}}" dir="ltr">${{amount}}</td>
            </tr>
            {% endfor %}
            <tr style="height: 31px">
                <td class="s0"></td>
                <td class="s22" dir="ltr"></td>
                <td class="s23" dir="ltr"></td>
                <td class="s24" dir="ltr">Total Hours</td>
                <td class="s25" dir="ltr">{{sums.sumHours}}</td>
                <td class="s26" dir="ltr">Subtotal</td>
                <td class="s27">${{sums.sumAmount}}</td>
            </tr>
            <tr style="height: 25px">
                <td class="s0"></td>
                <td class="s28" dir="ltr" colspan="4">Note: Please refer to attached Summary Report for details on the
                    invoiced work.</td>
                <td class="s26" dir="ltr">Adjustments</td>
                <td class="s29" dir="ltr">$0.00</td>
            </tr>
            <tr style="height: 39px">
                <td class="s30"></td>
                <td class="s31" dir="ltr"></td>
                <td class="s32"></td>
                <td class="s32"></td>
                <td class="s32"></td>
                <td class="s33" colspan="2">${{sums.sumAmount}}</td>
            </tr>
            <tr style="height: 25px">
                <td class="s3"></td>
                <td class="s34" dir="ltr" colspan="6"></td>
            </tr>
        </tbody>
    </table>
</div>